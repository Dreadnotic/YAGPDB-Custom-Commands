{{ else if and (not .User.Bot) (or (inFold (cslice "763074291297288212" "763074290940772392" "763074290567741481" "763074290450169876") (str .Reaction.Emoji.ID)) (eq .Reaction.Emoji.Name "üé≤")) }}
	{{ $dbRoll := dbGet .User.ID "roll" }}
