			{{ $roll1 := 0}}
			{{ if eq .Reaction.Emoji.Name "üé≤" }} {{ $roll1 = randInt 1 5 }}
			{{ else if eq (str .Reaction.Emoji.ID) "763074291297288212" }} {{ $roll1 = 1 }} 
